getBaseNotes() {
    return [E5, A5, A5, B5.8, C5.8, 
          C5.8, B5.8, C5.8, D5.8, E5.2, 
          A6, E5, C5, A5, 
          E5, D5, C5.8, B5.8, A5];
}


track_1() {
  c = [[[C5.1,C6.1,C4.1,C3.1,C2.1]]];     /* C octaves */
  b = c + 4;                              /* E octaves */
  r = c + 7;                              /* G octaves */
  a = 0;
  song = [];
  while (a < 12) {
    song = song :: (c+b+r) :: R.1->2 :: (c++ + b++ + r++) :: R.8->16 / 8;
    c += 1;
    b += 1;
    r += 1;
    a += 1;
  }
  song = song->3;
  return song;
  }



